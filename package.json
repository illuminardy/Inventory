{
  "author": "Yanardy",
  "name": "todo",
  "version": "1.0.0",
  "description": "Inventory",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run build-modules",
    "build-modules": "babel ./src ./api --out-dir ./lib",
    "clean": "rm -rf ./lib",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "dev": {
      "command": "node server.js",
      "env": {
        "DB_HOST": "localhost",
        "DB_PORT": "27017",
        "DB_NAME": "devDB"
      }
    },
    "test": {
      "command": "mocha",
      "env": {
        "DB_HOST": "localhost",
        "DB_PORT": "27017",
        "DB_NAME": "testDB"
      }
    }
  },
  "author": "Yanardy Sanchez",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.9",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "moment": "^2.17.1",
    "moment-range": "^2.2.0",
    "mongoose": "^4.7.2",
    "mongoose-currency": "^0.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-input-calendar": "^0.3.14",
    "react-redux": "^5.0.1",
    "react-table": "^3.1.4",
    "redux": "^3.6.0",
    "superagent": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.0",
    "node-sass": "^3.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0"
  }
}
